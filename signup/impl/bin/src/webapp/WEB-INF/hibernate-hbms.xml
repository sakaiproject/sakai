<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean
		id="org.sakaiproject.signup.hibernate.AdditionalHibernateMappings"
		class="org.sakaiproject.springframework.orm.hibernate.impl.AdditionalHibernateMappingsImpl">
		<property name="mappingResources">
			<list>
				<value>org/sakaiproject/signup/hbm/SignupMeeting.hbm.xml</value>
				<value>org/sakaiproject/signup/hbm/SignupSite.hbm.xml</value>
				<value>org/sakaiproject/signup/hbm/SignupTimeslot.hbm.xml</value>
			</list>
		</property>
	</bean>

</beans>
