	function resizeFrame(updown) 
	{
		var frame = parent.document.getElementById( window.name );
	
		if( frame ) 
		{
			if(updown=='shrink')
			{
				var clientH = document.body.clientHeight - 30;
			}
			else
			{
				var clientH = document.body.clientHeight + 30;
			}
			$( frame ).height( clientH );
		} 
	}
