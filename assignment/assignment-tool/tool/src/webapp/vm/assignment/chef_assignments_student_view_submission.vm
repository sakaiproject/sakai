						<p class="instruction">
							$tlang.getString("gen.orisub2")
						</p>
						<div class="textPanel">$cheffeedbackhelper.escapeAssignmentFeedback($text)</div>
						#end
						#elseif ($!submission.submittedText && ($submission.SubmittedText.length()>0))
							#set ($text = $submission.SubmittedText)
							#if ($!text)
								<div class="textPanel">$cheffeedbackhelper.escapeAssignmentFeedback($text)</div>
							#end
					#end
