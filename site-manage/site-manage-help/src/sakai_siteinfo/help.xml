<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
   <bean id="siteInfoAbout" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>argq</value></property>
      <property name="name"><value>Site Info</value></property>
      <property name="location"><value>/sakai_siteinfo/argq.html</value></property>
      <property name="defaultForTool"><value>sakai.siteinfo</value></property>
   </bean>

   <bean id="siteInfo" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqym</value></property>
      <property name="name"><value>Site Info Overview</value></property>
      <property name="location"><value>/sakai_siteinfo/aqym.html</value></property>
   </bean>
   
   <bean id="editASite" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>araw</value></property>
      <property name="name"><value>Edit Site</value></property>
      <property name="location"><value>/sakai_siteinfo/araw.html</value></property>
   </bean>

   <bean id="editNoTool" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arje</value></property>
      <property name="name"><value>Edit Site from Worksite Setup</value></property>
      <property name="location"><value>/sakai_siteinfo/arje.html</value></property>
   </bean>
   
   <bean id="editTools" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardx</value></property>
      <property name="name"><value>Add or remove tools</value></property>
      <property name="location"><value>/sakai_siteinfo/ardx.html</value></property>
   </bean>
   
   <bean id="manageGroups" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>atcs</value></property>
      <property name="name"><value>Manage Groups</value></property>
      <property name="location"><value>/sakai_siteinfo/atcs.html</value></property>
   </bean>
   
   <bean id="grantGlobalAccess" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arci</value></property>
      <property name="name"><value>Control Access</value></property>
      <property name="location"><value>/sakai_siteinfo/arci.html</value></property>
   </bean>
   
   <bean id="addRemoveParticipant" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arav</value></property>
      <property name="name"><value>Add/Edit/Delete Participant</value></property>
      <property name="location"><value>/sakai_siteinfo/arav.html</value></property>
   </bean>
   
   <bean id="participantRole" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardw</value></property>
      <property name="name"><value>Add Text Description</value></property>
      <property name="location"><value>/sakai_siteinfo/ardw.html</value></property>
   </bean>
   
   <bean id="changeParticipantRole" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arfb</value></property>
      <property name="name"><value>Change Participant Roles</value></property>
      <property name="location"><value>/sakai_siteinfo/arfb.html</value></property>
   </bean>
   
   <bean id="publishSite" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arai</value></property>
      <property name="name"><value>Publish/Unpublish Site</value></property>
      <property name="location"><value>/sakai_siteinfo/arai.html</value></property>
   </bean>
   
   <bean id="changeSiteAppearance" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardg</value></property>
      <property name="name"><value>Change Site Appearance</value></property>
      <property name="location"><value>/sakai_siteinfo/ardg.html</value></property>
   </bean>
   
   <bean id="addClass" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardu</value></property>
      <property name="name"><value>Add/Remove Class or Section</value></property>
      <property name="location"><value>/sakai_siteinfo/ardu.html</value></property>
   </bean>
   
   <bean id="importToSite" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arao</value></property>
      <property name="name"><value>Import from Site</value></property>
      <property name="location"><value>/sakai_siteinfo/arao.html</value></property>
   </bean>
   
   <bean id="viewSiteInfo" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqzx</value></property>
      <property name="name"><value>View Site Information</value></property>
      <property name="location"><value>/sakai_siteinfo/aqzx.html</value></property>
   </bean>
   
   <bean id="duplicateSite" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aral</value></property>
      <property name="name"><value>Duplicate Site</value></property>
      <property name="location"><value>/sakai_siteinfo/aral.html</value></property>
   </bean>

   <bean id="emailTools" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aueu</value></property>
      <property name="name"><value>Tools for managing email</value></property>
      <property name="location"><value>/sakai_siteinfo/aueu.html</value></property>
   </bean>

    <bean id="org.sakaiproject.api.app.help.TableOfContents" 
          class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
      <property name="name"><value>root</value></property>
      <property name="categories">
         <list>
            <bean id="siteInfoCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
               <property name="name"><value>Site Info</value></property>
               <property name="resources">
                  <list>
                     <ref bean="siteInfoAbout"/>
                     <ref bean="siteInfo"/>
                     <ref bean="editASite"/>
                     <ref bean="editNoTool"/>
                     <ref bean="editTools"/>
                     <ref bean="manageGroups"/>
                     <ref bean="grantGlobalAccess"/>
                     <ref bean="addRemoveParticipant"/>
                     <ref bean="participantRole"/>
                     <ref bean="changeParticipantRole"/>
                     <ref bean="publishSite"/>
                     <ref bean="changeSiteAppearance"/>
                     <ref bean="addClass"/>
                     <ref bean="importToSite"/>
                     <ref bean="viewSiteInfo"/>
                     <ref bean="duplicateSite"/>
                     <ref bean="emailTools"/>
                  </list>
               </property>
            </bean>
           </list>
         </property>
       </bean>
</beans>
