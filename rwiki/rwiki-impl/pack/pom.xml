  <name>sakai-rwiki-pack</name>
  <groupId>org.sakaiproject</groupId>
  <artifactId>sakai-rwiki-pack</artifactId>
  <description>
	Deployment of components 
  </description>
  <packaging>sakai-component</packaging>
  <properties>
    <deploy.target>components</deploy.target>
  </properties>
  <build>
    
    
    
    
    
    <resources>
      <resource>
        <directory>${basedir}/src/bundle</directory>
        <includes>
          <include>**/*.*</include>
        </includes>
        <filtering>false</filtering>
      </resource>
      <resource>
        <directory>${basedir}/src/java</directory>
        <includes>
          <include>**/*.xml</include>
        </includes>
        <filtering>false</filtering>
      </resource>
    </resources>
<!-- java source -->
    
    
    <testResources>
      <testResource>
        <directory>${basedir}/src/testBundle</directory>
        <includes>
          <include>**/*.*</include>
        </includes>
        <filtering>false</filtering>
      </testResource>
<!--includes: **/*Test -->
    </testResources>
<!-- other resources for the jar - the message bundles -->
  </build>
