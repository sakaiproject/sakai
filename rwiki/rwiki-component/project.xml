    <currentVersion>${sakai.version}</currentVersion>
  <shortDescription>Sakai RWiki Component Pack</shortDescription>
  <description>
	Deployment of components 
  </description>
  <properties>
	<deploy.type>components</deploy.type>
  </properties>
  <build>
    <nagEmailAddress>andrew@caret.cam.ac.uk</nagEmailAddress>
    <!-- java source -->
    <sourceDirectory>src/java</sourceDirectory>
    <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
    <unitTest>
      <includes>**/*Test</includes>
      <resources>
        <resource>
          <directory>${basedir}/src/testBundle</directory>
          <includes>
            <include>**/*.*</include>
          </includes>
          <filtering>false</filtering>
        </resource>
      </resources>
    </unitTest>
    <!-- other resources for the jar - the message bundles -->
    <resources>
      <resource>
        <directory>${basedir}/src/bundle</directory>
        <includes>
          <include>**/*.*</include>
        </includes>
        <filtering>false</filtering>
      </resource>
      <resource>
        <directory>${basedir}/src/java</directory>
        <includes>
          <include>**/*.xml</include>
        </includes>
        <filtering>false</filtering>
      </resource>
    </resources>
  </build>
