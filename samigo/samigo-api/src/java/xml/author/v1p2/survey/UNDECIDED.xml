        <fieldentry/>
      </qtimetadatafield>      
    </qtimetadata>
  </itemmetadata>
  <rubric/>
  <presentation label="Resp001">
    <flow>
      <material>
        <mattext/>
      </material>
      <material>
        <matimage imagtype="text/html" uri=""/>
      </material>
      <response_lid ident="TF02" rcardinality="Single">
        <render_choice shuffle="No">
          <response_label ident="A">
            <material>
              <mattext>Disagree</mattext>
            </material>
          </response_label>
          <response_label ident="B">
            <material>
              <mattext>Undecided</mattext>
            </material>
          </response_label>
          <response_label ident="C">
            <material>
              <mattext>Agree</mattext>
            </material>
          </response_label>
        </render_choice>
      </response_lid>
    </flow>
  </presentation>
  <resprocessing>
    <outcomes>
      <decvar defaultval="0" maxvalue="0" minvalue="0" varname="SCORE"/>
    </outcomes>
    <respcondition continue="No">
      <conditionvar>
        <other/>
      </conditionvar>
      <displayfeedback feedbacktype="Response" linkrefid="InCorrect"/>
    </respcondition>
  </resprocessing>
  <itemfeedback ident="Correct" view="All">
    <flow_mat>
      <material>
        <mattext/>
      </material>
      <material>
        <matimage imagtype="text/html" uri=""/>
      </material>
    </flow_mat>
  </itemfeedback>
  <itemfeedback ident="InCorrect" view="All">
    <flow_mat>
      <material>
        <mattext></mattext>
      </material>
    </flow_mat>
  </itemfeedback>
</item>
