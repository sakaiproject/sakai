        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[Which statement regarding the sodium-potassium pump is correct?<br>]]></mattext>
          </material>
          <response_lid ident="response" rcardinality="Single" rtiming="No">
            <render_choice shuffle="Yes">
              <response_label ident="A" rarea="Ellipse" rrange="Exact" rshuffle="Yes">
