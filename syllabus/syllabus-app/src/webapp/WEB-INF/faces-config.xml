      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>edit</from-outcome>
      <to-view-id>/syllabus/edit.jsp</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>main</from-outcome>
      <to-view-id>/syllabus/main.jsp</to-view-id>
      <redirect/>
    </navigation-case>
     <navigation-case>
      <from-outcome>edit_bulk</from-outcome>
