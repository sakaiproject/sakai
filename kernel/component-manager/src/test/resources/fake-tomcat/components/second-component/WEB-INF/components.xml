<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="secondBean" class="org.sakaiproject.modi.SpyBean">
        <property name="name"><value>Second Bean</value></property>
    </bean>

    <bean id="secondOverride" class="org.sakaiproject.modi.SpyBean">
        <property name="name"><value>original name</value></property>
    </bean>
</beans>