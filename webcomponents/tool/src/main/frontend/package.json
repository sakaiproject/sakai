{
  "name": "webcomponents",
  "version": "1.0.0",
  "description": "Sakai Element",
  "main": "js/sakai-element.js",
  "scripts": {
    "storybook": "npm install --force && empathise install --assets-directory=js/assets && start-storybook -p 6006 -s ./storybook-resources",
    "test-sakai-ui": "web-test-runner --config=\".config/web-test-runner.config.mjs\"",
    "build-storybook": "build-storybook",
    "sui:serve:storybook": "web-dev-server --config=\".config/web-test-runner.config.mjs\" --watch",
    "sui:test:mocha": "web-test-runner --config=\".config/web-test-runner.config.mjs\"",
    "eslint": "eslint -c .config/.eslintrc.js js/**/*.js",
    "wc:build": "CDN=$(git rev-parse --short=8  HEAD) npm-run-all wc:clean wc:copy-src wc:build:vite wc:copy-target",
    "wc:dev": "VITE_BUILD=development CDN=$(git rev-parse --short=8  HEAD) npm-run-all wc:clean wc:copy-src wc:build:vite wc:copy-target",
    "wc:build:vite": "vite build --config .config/vite.config.wc.js",
    "wc:build:maven": "npm-run-all wc:clean wc:copy-src wc:build:vite",
    "wc:serve:vite": "vite --config .config/vite.config.wc.js",
    "wc:watch": "VITE_BUILD=development CDN=$(git rev-parse --short=8  HEAD) chokidar \"./js/**/*\" -c \"npm run wc:dev\"",
    "wc:clean": "rimraf ./target",
    "wc:copy-src": "cpy './js/**/*' ./target",
    "wc:copy-target": "cpy './target/**/*' $TARGET"
  },
  "keywords": [],
  "author": "",
  "license": "ECL-2.0",
