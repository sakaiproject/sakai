<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN" "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
	<navigation-rule>
		<from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>studentView</from-outcome>
            <to-view-id>/studentView.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>overview</from-outcome>
            <to-view-id>/overview.jsp</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>closed</from-outcome>
            <to-view-id>/closed.jsp</to-view-id>
            <redirect/>
        </navigation-case>        
        <navigation-case>
            <from-outcome>editSection</from-outcome>
            <to-view-id>/editSection.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editStudentSections</from-outcome>
            <to-view-id>/editStudentSections.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteSections</from-outcome>
            <to-view-id>/deleteSections.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editManagers</from-outcome>
            <to-view-id>/editManagers.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editStudents</from-outcome>
            <to-view-id>/editStudents.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>roster</from-outcome>
            <to-view-id>/roster.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>addSections</from-outcome>
            <to-view-id>/addSections.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>options</from-outcome>
            <to-view-id>/options.jsp</to-view-id>
            <redirect/>
        </navigation-case>
	</navigation-rule>
</faces-config>
